{
  "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "experimentalObjectRestSpread": true
      }
  },
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
  },
  "extends": "eslint:recommended",
  "env": {
      "amd": true,
      "node": true,
      "browser": true,
      "es6": true
  }
}